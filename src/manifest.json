{
  "name": "WebDataMaster",
  "version": "0.5.1",
  "description": "Export and import web site data",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.ts"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/main.ts"]
    }
  ],
  "permissions": ["contextMenus", "storage"],
  "icons": {
    "16": "assets/16.png",
    "32": "assets/32.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  }
}
